    "@types/node": "^20.12.12",
    "ava": "^6.1.3",
    "debug": "^4.3.5",
    "esbuild": "^0.21.4",
    "esbuild-register": "^3.5.0",
    "kleur": "^4.1.5",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "@tscircuit/builder": "^1.5.109",
    "@tscircuit/props": "^0.0.6",
